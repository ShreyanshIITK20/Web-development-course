<%- include("partials/header") -%>     <!-- importing header file from a diff folder -->
<h1>Home</h1>
<p><%= startingContent  %> </p>
<%posts.forEach(function(post){%>
    <h1><%= post.title %> </h1>
    <p>
        <%= post.content.substring(0,100) +" ..." %> 
        <a href="/posts/<%= post.title  %>" >Read more</a>
    </p>
<%});%>
<%- include("partials/footer") -%> 